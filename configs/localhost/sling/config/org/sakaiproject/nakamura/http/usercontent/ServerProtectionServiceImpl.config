service.bundleLocation="slinginstall:org.sakaiproject.nakamura.http-0.11-SNAPSHOT.jar"
trusted.hosts=["http://localhost:8080"]
service.pid="org.sakaiproject.nakamura.http.usercontent.ServerProtectionServiceImpl"
trusted.paths=["/dev","/devwidgets","/system","/var/defaults"]
untrusted.contenturl="http://localhost:8082"
trusted.postwhitelist=["/system"]
trusted.exact.paths=["/","/index.html"]
