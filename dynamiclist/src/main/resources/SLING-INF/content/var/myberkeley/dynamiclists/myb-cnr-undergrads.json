{
  "sling:resourceType": "myberkeley/dynamicListContext",
  "myb-context": "myb-cnr-undergrads",
  "myb-clauses": [
    "/colleges/NAT RES/standings/undergrad",
    "/colleges/NAT RES/standings/undergrad/majors/*",
  ],
  "myb-filters": [
    "/student/*"
  ],
  "security:principals" : [
    {
      "name" : "myb-advisers-cnr-undergrads",
      "isgroup" :"true"
    }
  ],
  "security:acl" : [
    {
      "principal" : "everyone",
      "denied" : [
        "jcr:all"
      ]
    },
    {
      "principal" : "anonymous",
      "denied" : [
        "jcr:all"
      ]
    },
    {
      "principal" : "myb-advisers-cnr-undergrads",
      "granted" : [
        "jcr:read"
      ]
    }
  ]
}